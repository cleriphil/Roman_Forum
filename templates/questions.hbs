<div class="question-answer-box">

  <h1>Questions:</h1>
  <ul>
    {{#each}}
      <li>{{#link-to 'question' this.id}}{{question}}{{/link-to}}</li>
    {{/each}}
  </ul>
  <div class="plus-icon">

    <p>{{#link-to 'new-question' classNames="btn-default"}}<span class="glyphicon glyphicon-plus"></span>{{/link-to}}</p>
  </div>

</div>
